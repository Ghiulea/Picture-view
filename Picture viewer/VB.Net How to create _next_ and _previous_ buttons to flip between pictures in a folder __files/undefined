     <script type="text/javascript">
         function addAsyncScript(src) {
             var script = document.createElement('script');
             script.type = 'text/javascript';
             script.async = true;
             script.src = src;
             return script;
         };

         $(document).ready(function () {
             if (window.siteConsent) {
                 siteConsent.onConsentChanged(watchConsentChanges);
             }
             function getResetDate() {
                 var d = new Date();
                 d.setMonth(-12);
                 return d;
             }
             function watchConsentChanges(categoryPreferences) {
                 if (categoryPreferences.Analytics) {
                     document.getElementsByTagName('body')[0].appendChild(addAsyncScript("https://i1.social.s-msft.com/Forums/resources/scripts/webtrends.js?cver=0001"));
                 } else {
                     var expire = getResetDate();
                     document.cookie = "WT_FPC =; domain=.microsoft.com; expires=" + expire + "; path=/";
                 }
             }
         });
        </script>

    
